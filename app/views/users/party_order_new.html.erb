<td><%= i.submit "Add",:class=>"btn btn-info"%>
 <%end%></td></tr>

<% @party_order1.each do |i| %>
<tr>
<td><div class="fot"><%= i.party_order_list%></div></td>
<td><%= link_to image_tag("/assets/trash1.png", height: '20', width: '20',:class=>"img-circle",:title => 'Delete', :rel => 'tooltip',:onclick=>"if (confirm('Are you sure!!!')) commentDelete(1); return false"),{:action => 'party_order_delete', :id => i.id }%></td>

</tr>
<%end%>
</tbody>
</table>
</div>
</div>
<div class="digg_pagination" align="right">
    <%= will_paginate @party_order1, :container => false %>
  </div>
</div>
