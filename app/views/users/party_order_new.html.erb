
<div class="bod">
  <div class="col-md-10 col-sm-8 body-wrap">
<div class="set">
<div class="body-container">
<div class="body-header">
Order Party
</div>
<div class="body-content">
<div class="table-container">
<div class="header">
    List of Order Party
</div>
<table class="table">
    <thead>
<tr>
<th>Party - Order</th>
<th>Action</th>
</tr>
</thead>
<tbody>
<%= form_for([@party_order], validate: true, :url=>{:controller=>"users", :action => "party_order_create"}) do |i|%>
